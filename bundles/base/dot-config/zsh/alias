chmod=chmod --preserve-root
mkdir=mkdir -p
ls=lsd -av
ll=lsd -lav
lstree=lsd --tree
vim=nvim
pbcopy=xclip -i -sel c
pbpaste=xclip -o -sel -c
back=cd -
:q=exit
randomkey=openssl rand -base64 128 | tr -d '\n'
keybindings=bindkey -L
zshconfig=vim "$ZDOTDIR/.zshrc"
persona='echo "configuration for directory: $(pwd)\nsystem: $(whoami)\ngit: $(git config user.name)<$(git config user.email)>"'
